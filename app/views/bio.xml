<Alloy>
	<Window class="container">
		<View id="profileContainer">
			<View id="bioChange">
				<TextField class="content_text" id="bioTextField" hintText="Your Bio Goes Here...."></TextField>
				<TableView class="tableView">
					<TableViewRow id="row" layout="horizontal">
						<Button id="submitBio" onClick="submit">Submit</Button>
						<Button id="cancelBio" onClick="cancel">Cancel</Button>
					</TableViewRow>
				</TableView>
			</View>
		</View>		
	</Window>
</Alloy>